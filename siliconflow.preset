--https://docs.siliconflow.cn/en/api-reference/chat-completions/chat-completions
apibase="https://api.siliconflow.cn/v1"

--https://cloud.siliconflow.cn/me/account/ak
apikey=win.GetEnv"SILICONFLOW_API_KEY" or "set SILICONFLOW_API_KEY env variable!"

--https://cloud.siliconflow.cn/me/models?types=chat
model="deepseek-ai/DeepSeek-R1"

--https://docs.siliconflow.cn/en/api-reference/models/get-model-list
modelsMeta=[[{
  endpoint: "/models?sub_type=chat"
  filterFn: (model, models) ->
    models.list = models.list or {m.id,true for m in *models}
    models.list["Pro/"..model.id]
}]]

--https://docs.siliconflow.cn/en/api-reference/chat-completions/chat-completions#body-one-of-0-enable-thinking
extra_body={
  "enable_thinking: true, thinking_budget: 65368",
  "enable_thinking: true",
  "",
}
