--[[
curl "https://ai.gitee.com/api/pay/services?type=serverless&status=1&size=1000"
-- List of items tagged with "code-generation" or "text-generation"
jq "[.items[] | select(any(.tags[]?; .slug == \"code-generation\" or .slug ==\"text-generation\")) | .name] | sort"
-- With 0 price
jq "[.items[] | select(.operation_summary.min_price == 0) | .name] | sort"
jq "[.items[] | select(.operation_summary.min_price == 0 and any(.tags[]?; .slug == \"code-generation\" or .slug == \"text-generation\")) | .name] | sort"
-- Tags list
jq "[ .items[].tags[]? | .slug ] | unique | .[]"
jq "[ .items[].tags[]? | .slug | select(test(\"[0-9]\") | not) ] | unique | .[]"
-- Items without any tag
jq ".items[] | select((.tags // []) | length == 0) | .name"
--]]

